{
  "expo": {
    "name": "test",
    "slug": "test",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#013E23"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#013E23"
      },
      "splash":{
        "statusBar":{
          "visible": "false",
          "barStyle": "light-content",
          "backgroundColor": "#013E23"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
    
  }
}
